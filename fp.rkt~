(defvar l1 ())
 (define (reverse-general L)
   (define (reverse-general-helper-function (l1, L)
                                            (COND
                                             ((NULL? l) l1)
                                             (Else (reverse-general-helper-function (l1.append(car(l), cdr (l)))))))))
 
(reverse-general '(1 2 (3 4)))